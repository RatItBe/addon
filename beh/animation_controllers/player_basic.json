		"controller.animation.player.gamemode":{
			"initial_state":"default",
			"states":{
				"default":{
					"transitions":[
						{
							"survival":"q.scoreboard('gamemode') == 1"
						}
					]
				},
				"survival":{
					"on_entry": ["/scoreboard players set @s gamemode 0"],
					"on_exit": ["/gamemode s @s"],
					"transitions":[
						{
							"default":"q.scoreboard('gamemode') != 1"
						}
					]
				}
			}
